<div class="contenedor">
  <div class="signIn">Click para Ingresar</div>
    <div class="fold">

      <%= form_tag controller: :sessions, action: :create do %>
        <%= email_field_tag(:email, nil, placeholder: 'email') %>
        <%= password_field_tag(:password, nil, placeholder: 'password') %>
        <%= submit_tag("Ingresar") %>
      <% end %>

    </div>
</div>
